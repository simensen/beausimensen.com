<!DOCTYPE html>
<html lang="en">
<head>
    {% if page.full_title %}
    <title>{{ page.full_title|raw }}</title>
    {% else %}
    <title>{{ page.title|raw }} &mdash; {{ site.title|raw }}</title>
    {% endif %}
    {% block stylesheets %}
    {% include "stylesheets" %}
    {% endblock %}
    {% if page.description %}
    <meta name="description" content="{{ page.description }}" />
    {% else %}
    <meta name="description" content="Personal web site of Beau Simensen. A collection of his profiles, blogs, and other web sites." />
    {% endif %}
    <meta name="robots" content="INDEX,FOLLOW">
    <meta name="keywords" content="personal name, personal brand, interviewing, blogging, customer outreach, reach, resume, cv, hosting, personal site, family website">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <script src="{{ site.url }}/assets/js/libs/modernizr-custom.min.js"></script>
</head>
<body {% if page.bodyClass %} class="{{ page.bodyClass }}" {% endif %}>
<div class="content">
{% block content_wrapper %}{% block content %}Fallback content.{% endblock %}{% endblock %}
</div>
<script src="//ajax.googleapis.com/ajax/libs/jquery/1.8.0/jquery.min.js"></script>
<script>window.jQuery || document.write('<script src="{{ site.url }}/assets/js/libs/jquery-1.8.0.min.js"><\/script>')</script>
<script type="text/javascript">
var _gaq = _gaq || [];
_gaq.push(['_setAccount', 'UA-15693424-1']);
_gaq.push(['_trackPageview']);
(function() {
    var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
    ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
    var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
})();
</script>
</body>
</html>
